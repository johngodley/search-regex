{
	"name": "search-regex",
	"version": "3.0.4",
	"description": "Adds search and replace functionality across posts, pages, comments, and meta-data, with full regular expression support",
	"main": "search-regex.php",
	"sideEffects": true,
	"scripts": {
		"build": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:php": "composer run-script lint",
		"lint:psalm": "./vendor/vimeo/psalm/psalm --show-info=true",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:phan": "./vendor/bin/phan",
		"lint:stan": "./vendor/bin/phpstan analyse -c phpstan.neon",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip && mv search-regex.zip search-regex-$npm_package_version.zip",
		"release": "release-it",
		"start": "wp-scripts start",
		"test:js": "wp-scripts test-unit-js",
		"test:php": "composer run-script test",
		"locale": "rm -rf language/*.po language/*.mo language/json/*.json && composer run-script pot && yarn locale:download && yarn locale:json",
		"locale:download": "gulp potDownload",
		"locale:json": "gulp potJson",
		"doc": "apidoc -v -i api/ -o ./api-doc/",
		"svn": "gulp svn",
		"plugin": "gulp plugin"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johngodley/search-regex.git"
	},
	"author": "John Godley",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/johngodley/search-regex/issues"
	},
	"homepage": "https://github.com/johngodley/search-regex",
	"devDependencies": {
		"@types/jest": "^28.1.7",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@types/react-highlight-words": "^0.16.4",
		"@types/react-redux": "^7.1.24",
		"@wordpress/base-styles": "^4.7.0",
		"@wordpress/eslint-plugin": "^12.9.0",
		"@wordpress/prettier-config": "^1.4.0",
		"@wordpress/scripts": "^23.7.2",
		"apidoc": "^0.52.0",
		"chai": "^4.3.6",
		"download": "^8.0.0",
		"glob-all": "^3.3.0",
		"gulp": "^4.0.2",
		"gulp-deleted": "1.0.0",
		"gulp-po2json": "^1.0.0",
		"gulp-zip": "^5.1.0",
		"he": "^1.2.0",
		"jest": "^28.1.3",
		"jest-environment-jsdom": "^28.1.3",
		"mocha": "^10.0.0",
		"node-fetch": "2",
		"path": "^0.12.7",
		"prettier": "npm:wp-prettier@2.6.2",
		"release-it": "^15.3.0",
		"request": "^2.88.2",
		"through": "^2.3.8",
		"through2": "^4.0.2",
		"webpack-shell-plugin-next": "^2.2.2"
	},
	"dependencies": {
		"@emotion/react": "^11.10.0",
		"@redux-devtools/extension": "^3.2.3",
		"@wordpress/element": "^4.13.0",
		"@wordpress/i18n": "^4.15.0",
		"classnames": "^2.3.1",
		"date-fns": "^2.29.2",
		"deep-equal": "^2.0.5",
		"file-saver": "^2.0.5",
		"qs": "^6.11.0",
		"rc-progress": "^3.4.0",
		"rc-util": "^5.23.0",
		"react": "^18.2.0",
		"react-copy-to-clipboard": "^5.1.0",
		"react-datepicker": "^4.8.0",
		"react-delta": "^1.1.2",
		"react-dom": "^18.2.0",
		"react-dropzone": "^14.2.2",
		"react-focus-lock": "^2.9.1",
		"react-highlight-words": "^0.18.0",
		"react-redux": "^8.0.2",
		"react-textarea-autosize": "^8.3.4",
		"redux": "^4.2.0",
		"redux-thunk": "^2.4.1",
		"typescript": "^4.7.4",
		"url": "^0.11.0",
		"use-debounce": "^8.0.3"
	},
	"apidoc": {
		"name": "Search Regex REST API",
		"version": "1.0.0",
		"description": "WordPress REST API documentation for Search Regex",
		"title": "Search Regex REST API",
		"url": "https://searchregex.com/wp-json",
		"header": {
			"title": "Introduction",
			"filename": "./api-header.md"
		},
		"footer": {
			"filename": "./api-footer.md"
		},
		"template": {
			"withCompare": false
		},
		"sampleUrl": null
	},
	"wordpress": {
		"supported": "5.6"
	}
}
